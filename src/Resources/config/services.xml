<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
           https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults autowire="true" autoconfigure="true" />

        <service id="CustomPropertyMapper\ScheduledTask\ProductPropertyCheckTaskHandler" />

        <service id="CustomPropertyMapper\ScheduledTask\FrequentPropertyCheckTask" public="true">
            <tag name="shopware.scheduled.task" />
        </service>

        <service id="CustomPropertyMapper\ScheduledTask\FullPropertyCheckTask" public="true">
            <tag name="shopware.scheduled.task" />
        </service>

        <service id="CustomPropertyMapper\Service\PropertyMappingService" />
        <service id="CustomPropertyMapper\Admin\PropertyMappingController">
            <tag name="controller.service_arguments" />
        </service>

    </services>
</container>
